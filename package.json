{
  "name": "@stonejs-community/aws-lambda-adapter",
  "version": "0.0.1",
  "description": "Stone.js AWS Adapters",
  "author": "Mr. Stone <evensstone@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:stonejs-community/aws-lambda-adapter.git",
  "homepage": "https://github.com/stonejs-community/aws-lambda-adapter#readme",
  "bugs": {
    "url": "https://github.com/stonejs-community/aws-lambda-adapter/issues"
  },
  "keywords": [
    "aws",
    "faas",
    "node",
    "http",
    "lambda",
    "Stone.js",
    "middleware",
    "serverless"
  ],
  "files": [
    "/dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/types/config.d.ts",
      "default": "./dist/config.js"
    },
    "./decorators": {
      "types": "./dist/types/decorators.d.ts",
      "default": "./dist/decorators.js"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "types": "tsc",
    "lint": "standard src",
    "lint:fix": "standard --fix src tests",
    "lint:test": "standard --env jest tests",
    "predoc": "rimraf docs",
    "doc": "jsdoc src -r -a all -c jsdoc.json -R README.md -d docs/jsdoc",
    "doc:serve": "npm run doc && npx http-server docs/jsdoc -p 8081",
    "prebuild": "rimraf dist",
    "build": "npm run lint && rollup -c && npm run types",
    "release": "npm run test:text && npm run build && npm publish --access public",
    "local_publish": "npm run build && npm publish --registry http://localhost:4873/",
    "local_unpublish": "npm unpublish @stonejs-community/aws-lambda-adapter -f --registry http://localhost:4873",
    "test": "npm run lint:test && jest",
    "test:cvg": "npm run lint:test && jest --coverage",
    "test:text": "npm run test:cvg -- --coverageReporters=text",
    "test:html": "npm run test:cvg -- --coverageReporters=html text",
    "test:lcov": "npm run test:cvg -- --coverageReporters=lcov text"
  },
  "peerDependencies": {
    "@stone-js/adapters": "^0.0.1",
    "@stone-js/common": "^0.0.1",
    "@stone-js/http": "^0.0.1"
  },
  "dependencies": {
    "accepts": "^1.3.8",
    "bytes": "^3.1.2",
    "content-type": "^1.0.5",
    "deepmerge": "^4.3.1",
    "mime": "^3.0.0",
    "proxy-addr": "^2.0.7",
    "send": "^0.18.0",
    "statuses": "^2.0.1",
    "type-is": "^1.6.18"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "babel-jest": "^29.7.0",
    "docdash": "^2.0.2",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-node-externals": "^6.1.2",
    "standard": "^17.1.0",
    "typescript": "^5.2.2"
  },
  "stone": {
    "config": {
      "make": {
        "aws-lambda.adapter": "dist/options.mjs",
        "aws-lambda-http.adapter": "dist/options.http.mjs"
      }
    }
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "globals": [
      "it",
      "jest",
      "test",
      "expect",
      "describe",
      "afterEach",
      "beforeEach"
    ]
  }
}
